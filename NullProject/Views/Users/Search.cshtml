@using congress.DTOs
@model UsersDTO
<!-- begin::main content -->
<main class="main-content">

	<div class="card">
		<div class="card-body">
			<h6 class="card-title">یافتن کاربر</h6>
			<form class="needs-validation" method = "post" action= "/users/search" novalidate="">
				<input asp-for="NationalCode" hidden />
				<div class="form-row">
					<div class="col-md-4 mb-3">
						<label for="validationCustom01">کدملی</label>
						<input type="text" class="form-control" required="">
						<div class="valid-feedback">
							صحیح است!
						</div>
					</div>

				<button class="btn btn-primary" type="submit">جست و جو</button>
			</form>
		</div>
	</div>

</main>
<!-- end::main content -->
